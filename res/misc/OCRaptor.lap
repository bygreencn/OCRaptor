# janel settings

-DJAVA_HOME=res/bins/portable-java/win-x86-64/
-Djava.class.path=res/bins/ocraptor.jar
-Dfile.encoding=UTF-8

janel.jvm.path=res/bins/portable-java/win-x86-64/bin/server/jvm.dll
janel.java.home.path=res/bins/portable-java/win-x86-64/
